{
  "name": "the-hateful-eight",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:stats": "yarn run remove:dist && ng build --stats-json",
    "build:view:stats": "yarn run build:stats && yarn run view:stats",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "remove:dist": "rimraf dist",
    "start": "ng serve -o",
    "start:aot": "ng serve --aot -o",
    "test": "ng test",
    "test:changes": "ng test --onlyChanged",
    "test:coverage": "ng test --codeCoverage=true",
    "test:watch": "ng test --watch=true",
    "view:stats": "webpack-bundle-analyzer dist/stats-es5.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "moment": "^2.24.0",
    "ng-example-lib": "^0.0.4",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.0.2",
    "@angular-builders/jest": "^8.0.3",
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.15",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "webpack-bundle-analyzer": "~3.3.2"
  }
}
